<% provide :title, @title %>
<div class="col-md-8">
  <h3><%= @title %></h3>
  User_id : <%= @user.id %>
  There are : <%= @user.favorite_songs.count %> songs
  <ul class="users">
    <% if @users.any? %>
      <% @users.each do |user| %>
        <%= render user %>
        <!-- ren ra nut unfavorite -->
      <% end %>
    <% end %>
    <%= paginate @users %>
  </ul>
</div>
